
OUTPUT_ARCH(arm)
ENTRY(standby_main)

SECTIONS
{
	__standby_start = . ;
	.text 0xf0000000 :
	{
		drivers/soc/allwinner/pm/standby/main.o(.text.startup)
		*(.text)
		*(.rodata)
	}

	.data   : { *(.data) }
	__bss_start = . ;
	.bss : { *(.sbss) *(.scommon) *(.bss) *(COMMON) }
	__bss_end = .;
	__standby_end = . ;
}

